{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Date": {
      "type": "date",
      "required": true
    },
    "admin_users": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "admin::user"
    },
    "Cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Title": {
      "type": "string"
    },
    "PostSeo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo-information"
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title",
      "required": true
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "Content": {
      "type": "text",
      "required": true
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "posts"
    },
    "Description": {
      "type": "string",
      "required": true
    },
    "Media": {
      "allowedTypes": [
        "images",
        "videos",
        "audios",
        "files"
      ],
      "type": "media",
      "multiple": true
    }
  }
}
